{
  "builds": [
    { "src": "api/index.py", "use": "@vercel/python" },
    {
      "src": "web/package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    }
  ],
  "routes": [
    { "src": "^/api/(.*)$", "dest": "api/index.py" },
    { "handle": "filesystem" },
    { "src": "^(?!/api/).*", "dest": "index.html" }
  ]
}
